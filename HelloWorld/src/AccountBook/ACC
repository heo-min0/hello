
CREATE TABLE ACCOUNT01(
	ACCDATE DATE,
	ACCUSE VARCHAR2(30),
	ACCCLASSIFY VARCHAR2(30),
	ACCMONEY NUMBER,
	ACCMEMO VARCHAR2(50) );

DROP TABLE ACCOUNT01;
SELECT * FROM ACCOUNT01;
SELECT * FROM ACCOUNT01 WHERE TO_CHAR(ACCDATE, 'YYMMDD') = '210221';
INSERT INTO ACCOUNT01 VALUES(SYSDATE, '식비', '지출', 12000, '점심식사');
DELETE FROM ACCOUNT01 WHERE ACCMEMO = '점심식사';

SELECT * FROM EMPLOYEES WHERE HIRE_DATE IN '2003-06-17';